{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": "off",
        "import/no-extraneous-dependencies": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "prefer-template": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-undef": "off",
        "comma-dangle": "off"
    }
}
